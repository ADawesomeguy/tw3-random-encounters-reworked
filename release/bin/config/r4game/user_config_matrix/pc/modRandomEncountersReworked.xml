    <Group id="RandomEncountersMENU" displayName="Mods.modRE_title.modRE_main">
        <PresetsArray>
            <Preset id="0" displayName="modRE_vardefault" tags="customNames;customDisplayName">
                <Entry varId="enableTrophies"			value="0"	/>
				<Entry varId="citySpawn"				value="0"	/>
                <Entry varId="Difficulty"				value="1"	/>
            </Preset>
        </PresetsArray>
        <VisibleVars>	

            <Var id="enableTrophies" displayName="modRE_trophies" displayType="OPTIONS"
			tags="customNames;customDisplayName">
				<OptionsArray>
					<Option id="0" displayName="modRE_just_no">
						<Entry varId="enableTrophies" value="0"/>
					</Option>
					<Option id="1" displayName="modRE_just_yes">
						<Entry varId="enableTrophies" value="1"/>
					</Option>
				</OptionsArray>
			</Var>	
			
			<Var id="citySpawn" displayName="modRE_city" displayType="OPTIONS"
			tags="customNames;customDisplayName">
				<OptionsArray>
					<Option id="0" displayName="modRE_just_no">
						<Entry varId="citySpawn" value="0"/>
					</Option>
					<Option id="1" displayName="modRE_just_yes">
						<Entry varId="citySpawn" value="1"/>
					</Option>
				</OptionsArray>
			</Var>
			
			<Var id="cityBruxa" displayName="modRE_cityBruxa" displayType="OPTIONS"
			tags="customNames;customDisplayName">
				<OptionsArray>
					<Option id="0" displayName="modRE_just_no">
						<Entry varId="cityBruxa" value="0"/>
					</Option>
					<Option id="1" displayName="modRE_just_yes">
						<Entry varId="cityBruxa" value="1"/>
					</Option>
				</OptionsArray>
			</Var>
            
            <Var id="Difficulty" displayName="modRE_diff" displayType="OPTIONS" 
            tags="customNames;customDisplayName">
                <OptionsArray>
                    <Option id="0" displayName="modRE_easy">
                        <Entry varId="Difficulty" value="0"/>
                    </Option>
                    <Option id="1" displayName="modRE_med">
                        <Entry varId="Difficulty" value="1"/>
                    </Option>
                    <Option id="2" displayName="modRE_hard">
                        <Entry varId="Difficulty" value="2"/>
                    </Option>
                </OptionsArray>
            </Var>					
        </VisibleVars>
